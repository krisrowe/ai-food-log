# AI Food Logger Configuration
# Copy this file to config.ini and fill in your actual values
# config.ini is excluded from git for security

[gemini]
# Get your API key from: https://makersuite.google.com/app/apikey
api_key = your_gemini_api_key_here

[google_sheets]
# Google Sheets ID from the URL: https://docs.google.com/spreadsheets/d/{SHEET_ID}/edit
sheet_id = your_google_sheets_id_here

# Path to your Google service account JSON key file
# Download from: https://console.cloud.google.com/iam-admin/serviceaccounts
service_account_key = path/to/your/service-account-key.json

[database]
# SQLite database file (optional - will use Google Sheets if not specified)
# sqlite_file = food_log.db
